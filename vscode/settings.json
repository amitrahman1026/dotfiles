{
  "explorer.confirmDelete": false,
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "diffEditor.ignoreTrimWhitespace": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.automationShell.osx": "",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "C_Cpp.intelliSenseEngineFallback": "Enabled",
  "cph.language.cpp.Args": "-Wall -std=c++17",
  "security.workspace.trust.untrustedFiles": "open",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.codeAnalysis.clangTidy.args": ["-extra-arg=-std=c++17"],
  "C_Cpp.default.compilerPath": "/usr/bin/clang",
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Community Material Theme",
  "editor.formatOnSave": true,
  "C_Cpp.vcFormat.space.beforeInitializerListOpenBrace": true,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.renderWhitespace": "all",
  "remote.SSH.defaultExtensions": ["gitpod.gitpod-remote-ssh"],
  "workbench.startupEditor": "none",
  "arduino.useArduinoCli": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false
  },
  "githubPullRequests.pullBranch": "never",
  "python.formatting.provider": "black",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, UseTab: Never, IndentWidth: 4, TabWidth: 4, AllowShortIfStatementsOnASingleLine: true, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4, NamespaceIndentation: All, FixNamespaceComments: false }"
}
